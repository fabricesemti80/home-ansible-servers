# Cloudflare API Token (Replace with your actual token)
CLOUDFLARE_API_TOKEN=your-api-token

# Cloudflare Zone ID (Find it in your Cloudflare dashboard)
CLOUDFLARE_ZONE_ID=your-zone-id

# The domain/subdomain to update
DOMAIN=external.example.com

# Cloudflare Tunnel CNAME Target
TARGET=your-tunnel-id.cfargotunnel.com

# Cloudflare Tunnel ID (Replace with your actual tunnel ID)
CLOUDFLARE_TUNNEL_ID=your-tunnel-id

# Cron schedule (Default: every 5 minutes)
CRON_SCHEDULE=*/5 * * * *
